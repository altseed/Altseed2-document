{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "Src/Engine/**.csproj"
                    ]
                }
            ],
            "dest": "References",
            "filter": "filter.yml",
            "disableDefaultFilter": true,
            "properties": {
                "TargetFramework": "netstandard2.1"
            }
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md",
                    "References/**.yml",
                    "References/**.md",
                    "Manual/**.yml",
                    "Manual/**.md",
                    "Tutorials/**.yml",
                    "Tutorials/**.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "Images/**",
                    "Manual/**.png",
                    "Tutorials/**.png",
                    "Logo.png",
                    "favicon.ico"
                ]
            },
            {
                "src": "Style",
                "files": [
                    "*.css"
                ],
                "dest": "styles"
            }
        ],
        "overwrite": [
            {
                "files": [
                    "Overwrite/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "Site/**"
                ]
            }
        ],
        "globalMetadata": {
            "_appFooter": "Copyright (c) Altseed 2020.",
            "_disableContribution": true,
            "_appTitle": "Altseed2",
            "_enableSearch": true,
            "_appFaviconPath": "favicon.ico"
        },
        "dest": "Site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "mathew/src",
            "TemplateCustomize/"
        ],
        "postProcessors": [],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}